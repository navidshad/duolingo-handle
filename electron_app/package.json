{
  "name": "duolingo-handle",
  "productName": "Duolingo Handle",
  "version": "1.0.0",
  "description": "An exam hand for those who want to cheat on Duolingo exams",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start --env isDev=true",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make:mac": "electron-forge make --platform=darwin --arch=x64 --out=out/mac",
    "make:win": "electron-forge make --platform=win32 --arch=ia32 --out=out/win",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "keywords": [],
  "author": {
    "name": "A Developer"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^6.4.2",
    "@electron-forge/maker-deb": "^6.4.2",
    "@electron-forge/maker-rpm": "^6.4.2",
    "@electron-forge/maker-squirrel": "^6.4.2",
    "@electron-forge/maker-zip": "^6.4.2",
    "@electron-forge/plugin-webpack": "^6.4.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "copy-webpack-plugin": "^11.0.0",
    "electron": "24.0.0",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "node-abi": "3.51.0",
    "node-loader": "^2.0.0",
    "ts-loader": "^9.2.2",
    "ts-node": "^10.0.0",
    "typescript": "~4.5.4"
  },
  "dependencies": {
    "@google-cloud/speech": "^5.4.1",
    "@google-cloud/vision": "^3.1.2",
    "dotenv": "^16.0.3",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.1.0",
    "google-gax": "^3.6.0",
    "openai": "^3.2.1",
    "robotjs": "^0.6.0"
  }
}